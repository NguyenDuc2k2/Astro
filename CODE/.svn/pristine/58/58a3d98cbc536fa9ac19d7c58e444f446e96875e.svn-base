.article {
    padding: 10px 0;
    border-bottom: 1px dotted #001373;

    a {
      text-decoration: none;
      color: #23527c;

      &:visited {
        color: rgb(2, 139, 255);
        text-decoration: underline;
        outline: 0;
      }
    }

    img {
        display: block;
        width: auto;
        min-width: 100%;
    }
  }