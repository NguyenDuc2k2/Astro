.nav {
    @include element-center;
    @extend .bg-blue-black;
    color: $color-white;
    padding: 10px;
    gap: 20px;

    .btn-nav {
        font-size: 17px;
        padding: 3px;
        border-radius: 5px;

        &:hover {
            background-color: $color-white;
            color: $color-blue-black;
        }
    }
}